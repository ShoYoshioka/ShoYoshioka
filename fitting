    label54.grid(row = 5, column = 0)
    label55 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label55.grid(row = 5, column = 1)
    label56 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label56.grid(row = 5, column = 2)
    label57 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label57.grid(row = 5, column = 3)
    #frame6の場所を指定する。
    frame6.place(x=212,y=122)

    return bin4d, bin4s



def bin5fun(root, ttk, tk, bin5):  
    b5t = bin5['total']
    if b5t == '0':
        bin5d={
            '26.5': 0.0,
            '19.0': 0.0,
            '13.2': 0.0,
            '4.75': 0.0,
        }
        bin5s = {
            '26.5': 0.0,
            '19.0': 0.0,
            '13.2': 0.0,
            '4.75': 0.0,
        }
    else:
        bin5d = {
            '53.0': round(float(bin5['53.0'])*100/float(bin5['total']),1),
            '39.0': round(float(bin5['39.0'])*100/float(bin5['total']),1),
            '26.5': round(float(bin5['26.5'])*100/float(bin5['total']),1),
            '19.0': round(float(bin5['19.0'])*100/float(bin5['total']),1),
            '13.2': round(float(bin5['13.2'])*100/float(bin5['total']),1)
        }
        bin5s = {
            '53.0': round(100-float(bin5['53.0'])*100/float(bin5['total']),1),
            '39.0': round(100-float(bin5['39.0'])*100/float(bin5['total']),1),
            '26.5': round(100-float(bin5['26.5'])*100/float(bin5['total']),1),
            '19.0': round(100-float(bin5['19.0'])*100/float(bin5['total']),1),
            '13.2': round(100-float(bin5['13.2'])*100/float(bin5['total']),1)
        }


    #frame1を定義してbin5fun関数を出力した。
    frame1 = ttk.Frame(root)
    label4 = tk.Label(frame1, text ="5BIN", relief ="groove",width=27)
    label4.grid(row = 0, column = 0)
    frame1.place(x=1,y=80)
    #frame2を定義する。
    frame2 = ttk.Frame(root)
    #frame2の中にラベルを作る。
    label5 = tk.Label(frame2, text ="試料重量", relief ="groove",width=13)
    label5.grid(row = 0, column = 0)
    label6 = tk.Label(frame2, text ="0", relief ="groove",width=13)
    label6.grid(row = 0, column = 1)  
    #frame2の場所を指定する。
    frame2.place(x=2,y=101)

    #frame3を定義する。
    frame3 = ttk.Frame(root)
    #frame3の中にラベルを作る。
    label7 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label7.grid(row = 0, column = 0)
    label8 = tk.Label(frame3, text ="重量", relief ="groove",width=6)
    label8.grid(row = 0, column = 1)
    label9 = tk.Label(frame3, text ="残量", relief ="groove",width=6)
    label9.grid(row = 0, column = 2)
    label10 = tk.Label(frame3, text ="通過", relief ="groove",width=6)
    label10.grid(row = 0, column = 3)
    label11 = tk.Label(frame3, text ="53.0", relief ="groove",width=6)
    label11.grid(row = 1, column = 0)
    label12 = tk.Label(frame3, text =bin5["53.0"], relief ="groove",width=6)
    label12.grid(row = 1, column = 1)
    label13 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label13.grid(row = 1, column = 2)
    label14 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label14.grid(row = 1, column = 3)
    label15 = tk.Label(frame3, text ="39.0", relief ="groove",width=6)
    label15.grid(row = 2, column = 0)
    label16 = tk.Label(frame3, text =bin5["39.0"], relief ="groove",width=6)
    label16.grid(row = 2, column = 1)
    label17 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label17.grid(row = 2, column = 2)
    label18 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label18.grid(row = 2, column = 3)
    label19 = tk.Label(frame3, text ="26.5", relief ="groove",width=6)
    label19.grid(row = 3, column = 0)
    label20 = tk.Label(frame3, text =bin5["26.5"], relief ="groove",width=6)
    label20.grid(row = 3, column = 1)
    label21 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label21.grid(row = 3, column = 2)
    label22 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label22.grid(row = 3, column = 3)
    label23 = tk.Label(frame3, text ="19.0", relief ="groove",width=6)
    label23.grid(row = 4, column = 0)
    label24 = tk.Label(frame3, text =bin5["19.0"], relief ="groove",width=6)
    label24.grid(row = 4, column = 1)
    label25 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label25.grid(row = 4, column = 2)
    label26 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label26.grid(row = 4, column = 3)
    label27 = tk.Label(frame3, text ="13.2", relief ="groove",width=6)
    label27.grid(row = 5, column = 0)
    label28 = tk.Label(frame3, text =bin5["13.2"], relief ="groove",width=6)
    label28.grid(row = 5, column = 1)
    label29 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label29.grid(row = 5, column = 2)
    label30 = tk.Label(frame3, text =" ", relief ="groove",width=6)
    label30.grid(row = 5, column = 3)

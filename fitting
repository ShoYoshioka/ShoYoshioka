    #frame16の場所を指定する。
    frame16.place(x=250,y=270)
    '''
    「bin5d」「bin5s」「bin4d」「bin4s」はエラー回避のため空欄にした。
    '''
def synthesis(root,ttk,tk,bin1s,bin2s,bin3s,bin4s,bin5s,ishiko):
    #synthesis(合成粒度)の計算をする為の引数を入れる。


    synthesis = {
        '5bin':'0.0',
        '4bin':' ',
        '3bin':'22.0',
        '2bin':'33.3',
        '1bin':'44.4',
        'ishiko':'5.9',}



    bin3p = {
        '13.2': round(float(bin3s["13.2"])*(float(synthesis["3bin"])/100),1),
        '4.75': round(float(bin3s["4.75"])*(float(synthesis["3bin"])/100),1),
        '2.36': round(float(bin3s["2.36"])*(float(synthesis["3bin"])/100),1),
        }
    bin2p = {
        '4.75': round(float(bin2s["4.75"])*(float(synthesis["2bin"])/100),1),
        '2.36': round(float(bin2s["2.36"])*(float(synthesis["2bin"])/100),1),
        '0.60': round(float(bin2s["0.60"])*(float(synthesis["2bin"])/100),1),
        }
    bin1p = {
        '2.36': round(float(bin1s["2.36"])*(float(synthesis["1bin"])/100),1),
        '0.60': round(float(bin1s["0.60"])*(float(synthesis["1bin"])/100),1),
        '0.30': round(float(bin1s["0.30"])*(float(synthesis["1bin"])/100),1),
        '0.15': round(float(bin1s["0.15"])*(float(synthesis["1bin"])/100),1),
        '0.075': round(float(bin1s["0.075"])*(float(synthesis["1bin"])/100),1),
        }
    ishikop = {
        '0.15': round(float(ishiko["0.15"])*(float(synthesis["ishiko"])/100),1),
        '0.075': round(float(ishiko["0.075"])*(float(synthesis["ishiko"])/100),1),
        }
    grain = {
        "13.2": round(float(bin3p["13.2"])+float(synthesis["2bin"])+float(synthesis["1bin"])+float(synthesis["ishiko"]),1),
        '4.75': round(float(bin3p["4.75"])+float(bin2p["4.75"])+float(synthesis["1bin"])+float(synthesis["ishiko"]),1),
        '2.36': round(float(bin3p["2.36"])+float(bin2p["2.36"])+float(bin1p["2.36"])+float(synthesis["ishiko"]),1),
        '0.60': round(float(bin2p["0.60"])+float(bin1p["0.60"])+float(synthesis["ishiko"]),1),
        '0.30': round(float(bin1p["0.30"])+float(synthesis["ishiko"]),1),
        '0.15': round(float(bin1p["0.15"])+float(ishikop["0.15"]),1),
        '0.075':round(float(bin1p["0.075"])+float(ishikop["0.075"]),1),
        }
    
    #frame20を定義する。
    frame20 = ttk.Frame(root)

        '19.0': round(float(bin3['19.0'])*100/float(bin3['total']),1),
        '13.2': round(float(bin3['13.2'])*100/float(bin3['total']),1),
        '4.75': round(float(bin3['4.75'])*100/float(bin3['total']),1),
        '2.36': round(float(bin3['2.36'])*100/float(bin3['total']),1)
    }
    bin3s = {
        '19.0': round(100-float(bin3['19.0'])*100/float(bin3['total']),1),
        '13.2': round(100-float(bin3['13.2'])*100/float(bin3['total']),1),
        '4.75': round(100-float(bin3['4.75'])*100/float(bin3['total']),1),
        '2.36': round(100-float(bin3['2.36'])*100/float(bin3['total']),1)
    }


def bin4fun(root, ttk, tk, bin4):
    b4t = bin4['total']
    if b4t == '0':
        bin4d={
            '26.5': 0.0,
            '19.0': 0.0,
            '13.2': 0.0,
            '4.75': 0.0,
        }
        bin4s = {
            '26.5': 0.0,
            '19.0': 0.0,
            '13.2': 0.0,
            '4.75': 0.0,
        }
    else:
        bin4d = {
            '26.5': round(float(bin4['26.5'])*100/float(bin4['total']),1),
            '19.0': round(float(bin4['19.0'])*100/float(bin4['total']),1),
            '13.2': round(float(bin4['13.2'])*100/float(bin4['total']),1),
            '4.75': round(float(bin4['4.75'])*100/float(bin4['total']),1),
        }
        bin4s = {
            '2.36': round(100-float(bin4['26.5'])*100/float(bin4['total']),1),
            '19.0': round(100-float(bin4['19.0'])*100/float(bin4['total']),1),
            '13.2': round(100-float(bin4['13.2'])*100/float(bin4['total']),1),
            '4.75': round(100-float(bin4['4.75'])*100/float(bin4['total']),1),
        }


    '''
    引数だと「" "」などの空欄を反映できないので「bin5d」「bin5s」「bin4d」「bin4s」にはif文を付けて置いた。
    '''

    #frame4を定義してbin4fun関数を出力した。
    frame4 = ttk.Frame(root)
    label31 = tk.Label(frame4, text ="4BIN", relief ="groove",width=27)
    label31.grid(row = 0, column = 0)
    frame4.place(x=210,y=80)
    #frame5を定義する。
    frame5 = ttk.Frame(root)
    #frame5の中にラベルとテキストボックスを作る。
    label32 = tk.Label(frame5, text ="試料重量", relief ="groove",width=13)
    label32.grid(row = 0, column = 0)
    label33 = tk.Label(frame5, relief ="groove",width=13)
    label33.grid(row = 0, column = 1)
    #frame5の場所を指定する。
    frame5.place(x=211,y=101)
    #frame6を定義する。
    frame6 = ttk.Frame(root)
    #frame6の中にラベルを作る。
    label34 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label34.grid(row = 0, column = 0)
    label35 = tk.Label(frame6, text ="重量", relief ="groove",width=6)
    label35.grid(row = 0, column = 1)
    label36 = tk.Label(frame6, text ="残量", relief ="groove",width=6)
    label36.grid(row = 0, column = 2)
    label37 = tk.Label(frame6, text ="通過", relief ="groove",width=6)
    label37.grid(row = 0, column = 3)
    label38 = tk.Label(frame6, text ="26.5", relief ="groove",width=6)
    label38.grid(row = 1, column = 0)
    label39 = tk.Label(frame6, text =bin4["26.5"], relief ="groove",width=6)
    label39.grid(row = 1, column = 1)
    label40 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label40.grid(row = 1, column = 2)
    label41 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label41.grid(row = 1, column = 3)
    label42 = tk.Label(frame6, text ="19.0", relief ="groove",width=6)
    label42.grid(row = 2, column = 0)
    label43 = tk.Label(frame6, text =bin4["19.0"], relief ="groove",width=6)
    label43.grid(row = 2, column = 1)
    label44 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label44.grid(row = 2, column = 2)
    label45 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label45.grid(row = 2, column = 3)
    label46 = tk.Label(frame6, text ="13.2", relief ="groove",width=6)
    label46.grid(row = 3, column = 0)
    label47 = tk.Label(frame6, text =bin4["13.2"], relief ="groove",width=6)
    label47.grid(row = 3, column = 1)
    label48 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label48.grid(row = 3, column = 2)
    label49 = tk.Label(frame6, text =" ", relief ="groove",width=6)
    label49.grid(row = 3, column = 3)
    label50 = tk.Label(frame6, text ="4.75", relief ="groove",width=6)
    label50.grid(row = 4, column = 0)
    label51 = tk.Label(frame6, text =bin4["4.75"], relief ="groove",width=6)
    label51.grid(row = 4, column = 1)

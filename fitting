# -*- coding: utf-8 -*-
import tkinter as tk
import tkinter as ttk


def canvas():    #キャンバスを出力する。
    canvas = tk.Canvas(width = 700, height = 300, bg = "gray94")
    canvas.place(x=60, y=680)
    img = tk.PhotoImage(file="grain.png")
    canvas.create_image(350, 150, image=img)
    canvas.create_line(82, 252, 145, 236, fill="black",width=2)
    canvas.create_line(145, 236, 205, 198, fill="black",width=2)
    canvas.create_line(205, 198, 268, 170, fill="black",width=2)
    canvas.create_line(268, 170, 393, 140, fill="black",width=2)
    canvas.create_line(393, 140, 550, 11, fill="black",width=2)
    canvas.create_line(551, 11, 580, 8, fill="black",width=2) 


def title(root, tk):
    #タイトルを入れる。
    root.title("〇〇モジュール")
    root.geometry("850x1000")
    label1 = tk.Label(root, text="密粒度〇〇混合物(13)　AM10:00")  #文字ラベル設定
    label1.place(x=50,y=30) 
    label2 = tk.Label(root, text="試験月日　　　2019　年　09　月　12　日")  #文字ラベル設定
    label2.place(x=500,y=10) # 場所を指定

    #label3 = tk.Label(root, text="試験者　　　　　　高橋　雄央")  #文字ラベル設定
    #label3.place(x=500,y=30) # 場所を指定
    #label99の定義と場所指定をする。
    #label99= tk.Label(root,text="7.58",width=5)
    #label99.place(x=195,y=438)







def bin1fun(root,ttk, tk, bin1):
    bin1d = {
        '4.75': round(float(bin1['4.75'])*100/float(bin1['total']),1),
        '2.36': round(float(bin1['2.36'])*100/float(bin1['total']),1),
        '0.60': round(float(bin1['0.60'])*100/float(bin1['total']),1),
        '0.30': round(float(bin1['0.30'])*100/float(bin1['total']),1),
        '0.15': round(float(bin1['0.15'])*100/float(bin1['total']),1),
        '0.075': round(float(bin1['0.075'])*100/float(bin1['total']),1)
    }
    bin1s = {
        '4.75': round(100-float(bin1['4.75'])*100/float(bin1['total']),1),
        '2.36': round(100-float(bin1['2.36'])*100/float(bin1['total']),1),
        '0.60': round(100-float(bin1['0.60'])*100/float(bin1['total']),1),
        '0.30': round(100-float(bin1['0.30'])*100/float(bin1['total']),1),
        '0.15': round(100-float(bin1['0.15'])*100/float(bin1['total']),1),
        '0.075': round(100-float(bin1['0.075'])*100/float(bin1['total']),1)
    }
    
    #frame13を定義してbin1fun関数を出力した。
    frame13 = ttk.Frame(root)
    label139 = tk.Label(frame13, text ="1BIN", relief ="groove",width=27)
    label139.grid(row = 0, column = 0)
    frame13.place(x=1,y=270)
    #frame14を定義する。
    frame14 = ttk.Frame(root)
    #frame14の中にラベルを作る。
    label140 = tk.Label(frame14, text ="試料重量", relief ="groove",width=13)
    label140.grid(row = 0, column = 0)
    label141 = tk.Label(frame14, text =bin1["total"], relief ="groove",width=13)
    label141.grid(row = 0, column = 1)
    #frame14の場所を指定する。
    frame14.place(x=2,y=291)
    #frame15を定義する。
    frame15 = ttk.Frame(root)
    #frame15の中にラベルを作る。
    label142 = tk.Label(frame15, text =" ", relief ="groove",width=6)
    label142.grid(row = 0, column = 0)
    label143 = tk.Label(frame15, text ="重量", relief ="groove",width=6)
    label143.grid(row = 0, column = 1)
    label144 = tk.Label(frame15, text ="残量", relief ="groove",width=6)
    label144.grid(row = 0, column = 2)
    label145 = tk.Label(frame15, text ="通過", relief ="groove",width=6)
    label145.grid(row = 0, column = 3)
    label146 = tk.Label(frame15, text ='4.75', relief ="groove",width=6)
    label146.grid(row = 1, column = 0)
    label147 = tk.Label(frame15, text =bin1["4.75"], relief ="groove",width=6)
    label147.grid(row = 1, column = 1)
    label148 = tk.Label(frame15, text =bin1d["4.75"], relief ="groove",width=6)
    label148.grid(row = 1, column = 2)
    label149 = tk.Label(frame15, text =bin1s["4.75"], relief ="groove",width=6)
    label149.grid(row = 1, column = 3)
    label150 = tk.Label(frame15, text ="2.36", relief ="groove",width=6)
    label150.grid(row = 2, column = 0)
    label151 = tk.Label(frame15, text =bin1["2.36"], relief ="groove",width=6)
    label151.grid(row = 2, column = 1)
    label152 = tk.Label(frame15, text =bin1d["2.36"], relief ="groove",width=6)
    label152.grid(row = 2, column = 2)
    label153 = tk.Label(frame15, text =bin1s["2.36"], relief ="groove",width=6)
    label153.grid(row = 2, column = 3)
    label154 = tk.Label(frame15, text ="0.60", relief ="groove",width=6)
    label154.grid(row = 3, column = 0)
    label155 = tk.Label(frame15, text =bin1["0.60"], relief ="groove",width=6)
    label155.grid(row = 3, column = 1)
    label156 = tk.Label(frame15, text =bin1d["0.60"], relief ="groove",width=6)
    label156.grid(row = 3, column = 2)
    label157 = tk.Label(frame15, text =bin1s["0.60"], relief ="groove",width=6)
    label157.grid(row = 3, column = 3)
    label158 = tk.Label(frame15, text ="0.30", relief ="groove",width=6)
    label158.grid(row = 4, column = 0)
    label159 = tk.Label(frame15, text =bin1["0.30"], relief ="groove",width=6)
    label159.grid(row = 4, column = 1)
    label160 = tk.Label(frame15, text =bin1d["0.30"], relief ="groove",width=6)
    label160.grid(row = 4, column = 2)
    label161 = tk.Label(frame15, text =bin1s["0.30"], relief ="groove",width=6)
    label161.grid(row = 4, column = 3)
    label162 = tk.Label(frame15, text ="0.15", relief ="groove",width=6)
    label162.grid(row = 5, column = 0)
    label163 = tk.Label(frame15, text =bin1["0.15"], relief ="groove",width=6)
    label163.grid(row = 5, column = 1)
    label164 = tk.Label(frame15, text =bin1d["0.15"], relief ="groove",width=6)
    label164.grid(row = 5, column = 2)
    label165 = tk.Label(frame15, text =bin1s["0.15"], relief ="groove",width=6)
    label165.grid(row = 5, column = 3)
    label166 = tk.Label(frame15, text ="0.075", relief ="groove",width=6)
    label166.grid(row = 6, column = 0)
    label167 = tk.Label(frame15, text =bin1["0.075"], relief ="groove",width=6)
    label167.grid(row = 6, column = 1)
    label168 = tk.Label(frame15, text =bin1d["0.075"], relief ="groove",width=6)
    label168.grid(row = 6, column = 2)
    label169 = tk.Label(frame15, text =bin1s["0.075"], relief ="groove",width=6)
    label169.grid(row = 6, column = 3)
    #frame15の場所を指定する。
    frame15.place(x=3,y=312)

    return bin1d, bin1s



def bin2fun(root, ttk, tk, bin2):
    
    bin2d = {
        '13.2': round(float(bin2['13.2'])*100/float(bin2['total']),1),
        '4.75': round(float(bin2['4.75'])*100/float(bin2['total']),1),
        '2.36': round(float(bin2['2.36'])*100/float(bin2['total']),1),
        '0.60': round(float(bin2['0.60'])*100/float(bin2['total']),1)
    }
    bin2s = {
        '13.2': round(100-float(bin2['13.2'])*100/float(bin2['total']),1),
        '4.75': round(100-float(bin2['4.75'])*100/float(bin2['total']),1),

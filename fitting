    frame3.place(x=3,y=122)

    return bin5d, bin5s



def ishikofun(root, ttk, tk, ishiko):
    #ishiko(石粉)sum(合計)に引数を入れる。
    ishikosum = {
        '合計': round(float(ishiko['特殊'])+float(ishiko['6号'])+float(ishiko['7号'])+float(ishiko['砕砂'])
                    +float(ishiko['細砂'])+float(ishiko['5号'])+float(ishiko['4号'])+float(ishiko['4.75'])
                    +float(ishiko['2.36'])+float(ishiko['0.60'])+float(ishiko['0.30'])+float(ishiko['0.15'])
                    +float(ishiko['0.075']))}
